{
  "rules": [
    {"name": "subr-whole", "content": "/\\(([^()]+|(?R))*\\)/"},
    {"name": "subr-absolute1", "content": "/a(b(?1)?y)z/"},
    {"name": "subr-absolute2", "content": "/a(b\\g<1>?y)z/"},
    {"name": "subr-absolute3", "content": "/a(b\\g'1'?y)z/"},
    {"name": "subr-relplus1", "content": "/(?+1)x([ab])/"},
    {"name": "subr-relplus2", "content": "/\\g<+1>x([ab])/"},
    {"name": "subr-relplus3", "content": "/\\g'+1'x([ab])/"},
    {"name": "subr-relminus1", "content": "/a(b(?-1)?y)z/"},
    {"name": "subr-relminus2", "content": "/a(b\\g<-1>?y)z/"},
    {"name": "subr-relminus3", "content": "/a(b\\g'-1'?y)z/"},
    {"name": "subr-name1", "content": "/a(?<x>b(?&x)?y)z/"},
    {"name": "subr-name2", "content": "/a(?P<x>b(?P>x)?y)z/"},
    {"name": "subr-name3", "content": "/a(?<x>b\\g<x>?y)z/"},
    {"name": "subr-name4", "content": "/a(?'x'b\\g'x'?y)z/"}
  ],

  "grammars": [
    {"name": "subr-whole"},
    {"name": "subr-absolute1"},
    {"name": "subr-absolute2"},
    {"name": "subr-absolute3"},
    {"name": "subr-relplus1"},
    {"name": "subr-relplus2"},
    {"name": "subr-relplus3"},
    {"name": "subr-relminus1"},
    {"name": "subr-relminus2"},
    {"name": "subr-relminus3"},
    {"name": "subr-name1"},
    {"name": "subr-name2"},
    {"name": "subr-name3"},
    {"name": "subr-name4"}
  ],

  "patterns": [
    {"name": "subr-whole", "content": "(((abcde)))"},
    {"name": "subr-absolute1", "content": "abyzaaabyzzaabbyyz"},
    {"name": "subr-absolute2", "content": "abyzaaabyzzaabbyyz"},
