{
  "rules": [
    {"name":"char_type-dot", "content": "/abc./"},
    {"name":"char_type-code_unit", "content": "/123\\C/"},
    {"name":"char_type-digit", "content": "/abc\\d/"},
    {"name":"char_type-non_digit", "content": "/abc\\D123/"},
    {"name":"char_type-horiz_ws", "content": "/abc\\h123/"},
    {"name":"char_type-non_horiz_ws", "content": "/abc\\H123/"},
    {"name":"char_type-non_newline", "content": "/abc\\N123/"},
    {"name":"char_type-newline_seq", "content": "/abc\\Rabc/"},
    {"name":"char_type-ws", "content": "/abc\\sabc/"},
    {"name":"char_type-non_ws", "content": "/abc\\Sabc/"},
    {"name":"char_type-vs", "content": "/abc\\vabc/"},
    {"name":"char_type-non_vs", "content": "/abc\\Vabc/"},
    {"name":"char_type-word", "content": "/abc\\wabc/"},
    {"name":"char_type-non_word", "content": "/abc\\Wabc/"}
  ],

  "grammars": [
    {"name":"char_type-dot"},
    {"name":"char_type-code_unit"},
    {"name":"char_type-digit"},
    {"name":"char_type-non_digit"},
    {"name":"char_type-horiz_ws"},
    {"name":"char_type-non_horiz_ws"},
    {"name":"char_type-non_newline"},
    {"name":"char_type-newline_seq"},
    {"name":"char_type-ws"},
    {"name":"char_type-non_ws"},
    {"name":"char_type-vs"},
    {"name":"char_type-non_vs"},
    {"name":"char_type-word"},
    {"name":"char_type-non_word"}
  ],

  "patterns": [
    {"name":"char_type-dot", "content": "abcdef"},
    {"name":"char_type-code_unit", "content": "abc123abc"},
    {"name":"char_type-digit", "content": "xyzabc0a"},
    {"name":"char_type-non_digit", "content": "abcd123"},
    {"name":"char_type-horiz_ws", "content": "xyabc 123def"},
    {"name":"char_type-non_horiz_ws", "content": "xyabcd12345"},
    {"name":"char_type-non_newline", "content": "xyabcd12345"},
    {"name":"char_type-newline_seq", "content": "abc\\x0Aabc", "ctype": "hex"},
    {"name":"char_type-ws", "content": "123abc abc123"},
    {"name":"char_type-non_ws", "content": "123abc!abc123"},
    {"name":"char_type-vs", "content": "123 abc\nabc 123"},
    {"name":"char_type-non_vs", "content": "123abc abc123"},
    {"name":"char_type-word", "content": "123abcSabc123"},
    {"name":"char_type-non_word", "content": "123abc!abc123"}
  ],

  "engines" : ["pcre", "rematch", "hyperscan"],
  "results" : ["match", "nomatch", "error"],

  "tests" : [
     {"rule":"char_type-dot",
      "pattern": "char_type-dot",
      "grammars":["char_type-dot"], "expect": "match" },
     {"rule":"char_type-code_unit",
      "pattern": "char_type-code_unit",
      "grammars":["char_type-code_unit"], "expect": "match" },
     {"rule":"char_type-digit",
      "pattern": "char_type-digit",
      "grammars":["char_type-digit"], "expect": "match" },
     {"rule":"char_type-non_digit",
      "pattern": "char_type-non_digit",
      "grammars":["char_type-non_digit"], "expect": "match" },
     {"rule":"char_type-horiz_ws",
      "pattern": "char_type-horiz_ws",
      "grammars":["char_type-horiz_ws"], "expect": "match" },
     {"rule":"char_type-non_horiz_ws",
      "pattern": "char_type-non_horiz_ws",
      "grammars":["char_type-non_horiz_ws"], "expect": "match" },
     {"rule":"char_type-non_newline",
      "pattern": "char_type-non_newline",
      "grammars":["char_type-non_newline"], "expect": "match" },
     {"rule":"char_type-newline_seq",
      "pattern": "char_type-newline_seq",
      "grammars":["char_type-newline_seq"], "expect": "match" },
     {"rule":"char_type-ws",
      "pattern": "char_type-ws",
      "grammars":["char_type-ws"], "expect": "match" },
     {"rule":"char_type-non_ws",
      "pattern": "char_type-non_ws",
      "grammars":["char_type-non_ws"], "expect": "match" },
     {"rule":"char_type-vs",
      "pattern": "char_type-vs",
      "grammars":["char_type-vs"], "expect": "match" },
     {"rule":"char_type-non_vs",
      "pattern": "char_type-non_vs",
      "grammars":["char_type-non_vs"], "expect": "match" },
     {"rule":"char_type-word",
      "pattern": "char_type-word",
      "grammars":["char_type-word"], "expect": "match" },
     {"rule":"char_type-non_word",
      "pattern": "char_type-non_word",
      "grammars":["char_type-non_word"], "expect": "match" }
  ]
}
